<div id="navcontainer">
  <ul id="navlist">
    <%= render :partial => 'navigation/topmenu_item', 
        :locals => { :menu => 'Main Menu', :path => loads_path, :submenu_items => main_submenu_items } %>

    <%= render :partial => 'navigation/topmenu_item', 
        :locals => { :menu => 'Maintenance', :path => aircrafts_path, :submenu_items => maintenance_submenu_items } %>

    <div id="user_nav">
      <% if current_user -%>
        Logged in as <%= link_to current_user.username, edit_user_path(:current) %> |
        <%= link_to 'Logout', logout_path %>
      <% end -%>
    </div>
  </ul>
</div>
