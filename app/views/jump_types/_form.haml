- form_for(@jump_type) do |f|
  = f.error_messages

  %p
    = f.label :name, 'Name:'
    = f.text_field :name

  %p
    = f.label :cost, 'Cost:'
    = f.text_field :cost

  %p
    = f.label :default_altitude, 'Default Altitude:'
    = f.text_field :default_altitude

  %p
    = f.submit 'Submit'

= set_focus_to_id 'jump_type_name'
